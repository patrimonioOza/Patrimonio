<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".patrimonio_natural">

    <ImageButton
        android:id="@+id/logoTeo"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="#00FFFFFF"
        android:contentDescription="botonTeo"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_teo_con_nombre" />

    <ImageButton
        android:id="@+id/hamburguesa"
        android:layout_width="48dp"
        android:layout_height="55dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="#00FFFFFF"
        android:contentDescription="hamburguesa"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hamburguesa" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/patrimonio"
                android:layout_width="191dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:background="#FFFFFF"
                android:gravity="center"
                android:text="PATRIMONIO NATURAL"
                android:textColor="#415419"
                android:textSize="24sp"
                android:textStyle="bold" />


            <ImageButton
                android:id="@+id/boton4"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="miradoiro_cumio"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/miradoiro_do_cumio_de_montouto_e_pena_escorredia0" />

            <ImageButton
                android:id="@+id/boton5"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="miradoiro_pena_agrela"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/miradoiro_na_pena_de_agrela0" />

            <ImageButton
                android:id="@+id/boton6"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="carballeira_francos"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/carballeira_de_francos0" />

            <ImageButton
                android:id="@+id/boton7"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="sistema_fluvial_ulla_deza"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/sistema_fluvial_ulla_deza_rede_natura_2000_0" />

            <ImageButton
                android:id="@+id/boton8"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="couro_pesca_xirimbao"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/couto_de_pesca_sen_morte_do_xirimbao0" />

            <ImageButton
                android:id="@+id/boton9"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="area_recreativa_xirimbao"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/area_recreativa_do_xirimbao0" />

            <ImageButton
                android:id="@+id/boton10"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="fraga_sestelo"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/fraga_do_sestelo0" />

            <ImageButton
                android:id="@+id/boton11"
                android:layout_width="match_parent"
                android:layout_height="287dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:background="#00FFFFFF"
                android:contentDescription="devesa_agromaior"
                android:scaleType="fitXY"
                android:foreground="?android:attr/selectableItemBackground"
                android:theme="@android:style/ThemeOverlay.Material.Dark"
                app:srcCompat="@drawable/devesa_de_agromaior0" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="362dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="70dp"
                android:foregroundTint="#2DFFFFFF"
                app:srcCompat="@drawable/lineaverde" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">


                <ImageButton
                    android:id="@+id/facebook_3_"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    app:tint="#7F000000"
                    android:background="@drawable/animafacebook"
                    app:srcCompat="@drawable/facebook_3_" />


                <ImageButton
                    android:id="@+id/twitter_4_"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:contentDescription="twitter"
                    android:scaleType="fitXY"
                    android:background="@drawable/animatwitter"
                    app:srcCompat="@drawable/twitter_4_"
                    app:tint="#00B8AEAE" />


                <ImageButton
                    android:id="@+id/instagram_3_"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    app:tint="#7F000000"
                    android:background="@drawable/animainsta"
                    app:srcCompat="@drawable/instagram_3_"
                    />


                <ImageButton
                    android:id="@+id/informacion"
                    android:layout_width="55dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="175dp"
                    android:layout_marginTop="-5dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="#00B8AEAE"
                    android:contentDescription="informaciÃ³n"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/informacion"
                    app:tint="#7F000000" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>